<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AIModels\model.onnx" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="AIModels\model.onnx">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <LogicalName>%(RecursiveDir)%(Filename)%(Extension)</LogicalName>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ML.OnnxRuntime.DirectML" Version="1.23.0" />
    <PackageReference Include="NUlid" Version="1.7.3" />
    <PackageReference Include="Radzen.Blazor" Version="8.3.1" />
  </ItemGroup>


</Project>
